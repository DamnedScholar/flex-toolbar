{
  "name": "flex-tool-bar",
  "main": "./lib/main.js",
  "version": "0.12.0",
  "description": "Easily Customizable ToolBar for Atom",
  "repository": "https://github.com/cakecatz/flex-toolbar",
  "license": "MIT",
  "keywords": [
    "toolbar",
    "tool-bar"
  ],
  "engines": {
    "atom": ">=0.188.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.6.0",
    "change-case": "^3.0.0",
    "chokidar": "^1.7.0",
    "cson": "^3.0.2",
    "event-kit": "^2.3.0",
    "fs-extra": "^3.0.1",
    "json5": "^0.5.0",
    "space-pen": "^5.1.1",
    "tree-match-sync": "0.0.2"
  },
  "package-deps": [
    "tool-bar"
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "remark": "^4.1.2",
    "remark-lint": "^3.0.0"
  },
  "configSchema": {
    "toolBarConfigurationFilePath": {
      "type": "string",
      "description": "",
      "default": ""
    },
    "reloadToolBarWhenEditConfigFile": {
      "type": "boolean",
      "default": true
    },
    "useBrowserPlusWhenItIsActive": {
      "type": "boolean",
      "default": false
    }
  }
}
